    ## Suggested values include: 1] main; 2] sub (subordinate); 3] alt (alternate); 4] short; 5] desc (descriptive)
    [ a:defaultValue = "main" ]
    attribute type {
      ## main title of the work
      "main" | ## (subordinate) subtitle of the work
        "sub" | ## (alternate) alternative title of the work
        "alt" | ## abbreviated form of title
        "short" | ## (descriptive) descriptive paraphrase of the work
        "desc"
      | xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }

