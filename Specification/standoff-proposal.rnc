     (standoff-proposal_model.divTop,
      (standoff-proposal_model.global
       | standoff-proposal_model.divTop)*)?,
     (standoff-proposal_model.divGenLike,
      (standoff-proposal_model.global
       | standoff-proposal_model.divGenLike)*)?,
     ((standoff-proposal_model.divLike,
       (standoff-proposal_model.global
        | standoff-proposal_model.divGenLike)*)+
      | (standoff-proposal_model.div1Like,
         (standoff-proposal_model.global
          | standoff-proposal_model.divGenLike)*)+
      | ((standoff-proposal_model.common,
          standoff-proposal_model.global*)+,
         ((standoff-proposal_model.divLike,
           (standoff-proposal_model.global
            | standoff-proposal_model.divGenLike)*)+
          | (standoff-proposal_model.div1Like,
             (standoff-proposal_model.global
              | standoff-proposal_model.divGenLike)*)+)?)),
     (standoff-proposal_model.divBottom,
      standoff-proposal_model.global*)*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.declaring.attributes,
    empty
  }

standoff-proposal_group =
  ## contains the body of a composite text, grouping together a sequence of distinct texts (or groups of such texts) which are regarded as a unit for some purpose, for example the collected works of an author, a sequence of prose essays, etc. [4. Default Text Structure 4.3.1. Grouped Texts 15.1. Varieties of Composite Text]
  element tei:group {
    ((standoff-proposal_model.divTop | standoff-proposal_model.global)*,
     ((standoff-proposal_text | standoff-proposal_group),
      (standoff-proposal_text
       | standoff-proposal_group
       | standoff-proposal_model.global)*),
