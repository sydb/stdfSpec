    standoff-proposal_att.resourced.attributes,
    standoff-proposal_att.declaring.attributes,
    standoff-proposal_att.timed.attributes,
    ## (MIME media type) specifies the applicable multimedia internet mail extension (MIME) media type
    attribute mimeType {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    },
    empty
  }

standoff-proposal_graphic =
  ## indicates the location of a graphic or illustration, either forming part of a text, or providing an image of it. [3.9. Graphics and Other Non-textual Components 11.1. Digital Facsimiles]
  element tei:graphic {
    standoff-proposal_model.descLike*,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.media.attributes,
    standoff-proposal_att.resourced.attributes,
    standoff-proposal_att.declaring.attributes,
    empty
  }

standoff-proposal_binaryObject =
  ## provides encoded binary data representing an inline graphic, audio, video or other object. [3.9. Graphics and Other Non-textual Components]
  element tei:binaryObject {
