    ((standoff-proposal_model.glossLike
      | standoff-proposal_model.descLike)*,
     standoff-proposal_constraint?)
    >> sch:ns [
         prefix = "s"
         uri = "http://www.ascc.net/xml/schematron"
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-constraintSpec-sch_no_more-constraint-report-34"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:constraintSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "tei:constraint/s:*  and  @scheme = ('isoschematron','schematron')"
             "Rules\x{a}" ~
             "        in the Schematron 1.* language must be inside a constraintSpec\x{a}" ~
             "        with a value other than 'schematron' or 'isoschematron' on the\x{a}" ~
             "        scheme attribute"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:ns [
         prefix = "sch"
         uri = "http://purl.oclc.org/dsdl/schematron"
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-constraintSpec-isosch-constraint-report-35"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:constraintSpec"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "tei:constraint/sch:*  and  not( @scheme eq 'schematron')"
             "Rules\x{a}" ~
             "        in the ISO Schematron language must be inside a constraintSpec\x{a}" ~
             "        with the value 'schematron' on the scheme attribute"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:ns [
         prefix = "sch"
         uri = "http://purl.oclc.org/dsdl/schematron"
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-constraintSpec-needrules-constraint-rule-22"
         "\x{a}" ~
         "            "
         sch:rule [
           context =
             "tei:macroSpec/tei:constraintSpec[@scheme eq 'schematron']/tei:constraint"
           "\x{a}" ~
           "               "
           sch:report [
             test = "sch:assert|sch:report"
             "An ISO Schematron constraint specification for a macro should not\x{a}" ~
             "        have an 'assert' or 'report' element without a parent 'rule' element"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.identified.attributes,
    standoff-proposal_att.typed.attribute.subtype,
    ## characterizes the constraintSpec element in some sense; used to indicate when a constraintSpec warns about a deprecated construct.
    ## Suggested values include: 1] deprecationWarning
    attribute type {
      ## Indicates that this constraint specification warns that some other construct in the schema is deprecated.
      "deprecationWarning"
      | xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    ## supplies the name of the language in which the constraints are defined
    ## Suggested values include: 1] schematron (ISO Schematron)
    attribute scheme {
      ## (ISO Schematron) 
      "schematron"
      | xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    },
    empty
  }

standoff-proposal_attList =
  ## contains documentation for all the attributes associated with this element, as a series of attDef elements. [22.5. Element Specifications 22.6. Class Specifications]
  element tei:attList {
    (standoff-proposal_attRef
     | standoff-proposal_attDef
     | standoff-proposal_attList)+,
    standoff-proposal_att.global.attributes,
    ## (organization) specifies whether all the attributes in the list are available (org="group") or only one of them (org="choice")
    [ a:defaultValue = "group" ]
    attribute org {
      ## grouped
      "group" | ## alternated
        "choice"
    }?,
    empty
  }

standoff-proposal_attDef =
  ## (attribute definition) contains the definition of a single attribute. [22.5.3. Attribute List Specification]
  element tei:attDef {
    ((standoff-proposal_model.glossLike
      | standoff-proposal_model.descLike)*,
     standoff-proposal_datatype?,
     standoff-proposal_constraintSpec*,
     standoff-proposal_defaultVal?,
     (standoff-proposal_valList | standoff-proposal_valDesc+)?,
     standoff-proposal_exemplum*,
     standoff-proposal_remarks*)
    >> sch:ns [
         prefix = "teix"
         uri = "http://www.tei-c.org/ns/Examples"
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-attDef-attDefContents-constraint-assert-37"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:attDef"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "ancestor::teix:egXML[@valid='feasible'] or @mode eq 'change' or @mode eq 'delete' or tei:datatype or tei:valList[@type='closed']"
             "Attribute: the definition of the @"
             sch:value-of [ select = "@ident" ]
             " attribute in the "
             sch:value-of [ select = "ancestor::*[@ident][1]/@ident" ]
             "\x{a}" ~
             "                  "
             sch:value-of [ select = "' '" ]
             "\x{a}" ~
             "                  "
             sch:value-of [
               select = "local-name(ancestor::*[@ident][1])"
             ]
             " should have a closed valList or a datatype"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-attDef-noDefault4Required-constraint-rule-23"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:attDef[@usage eq 'req']"
           "\x{a}" ~
           "               "
           sch:report [
             test = "tei:defaultVal"
             'It does not make sense to make "'
             sch:value-of [ select = "normalize-space(tei:defaultVal)" ]
             '" the default value of @'
             sch:value-of [ select = "@ident" ]
             ", because that attribute is required."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-attDef-defaultIsInClosedList-twoOrMore-constraint-rule-24"
         "\x{a}" ~
         "            "
         sch:rule [
           context =
             "tei:attDef[   tei:defaultVal   and   tei:valList[@type eq 'closed']   and   tei:datatype[    @maxOccurs > 1    or    @minOccurs > 1    or    @maxOccurs = 'unbounded'    ]   ]"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "     tokenize(normalize-space(tei:defaultVal),' ')     =     tei:valList/tei:valItem/@ident"
             "In the "
             sch:value-of [
               select = "local-name(ancestor::*[@ident][1])"
             ]
             " defining\x{a}" ~
             "        "
             sch:value-of [ select = "ancestor::*[@ident][1]/@ident" ]
             " the default value of the\x{a}" ~
             "        @"
             sch:value-of [ select = "@ident" ]
             " attribute is not among the closed list of possible\x{a}" ~
             "        values"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-attDef-defaultIsInClosedList-one-constraint-rule-25"
         "\x{a}" ~
         "            "
         sch:rule [
           context =
             "tei:attDef[   tei:defaultVal   and   tei:valList[@type eq 'closed']   and   tei:datatype[    not(@maxOccurs)    or (    if ( @maxOccurs castable as xs:integer )     then ( @maxOccurs cast as xs:integer eq 1 )     else false()    )]   ]"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "string(tei:defaultVal)      =      tei:valList/tei:valItem/@ident"
             "In the "
             sch:value-of [
               select = "local-name(ancestor::*[@ident][1])"
             ]
             " defining\x{a}" ~
             "        "
             sch:value-of [ select = "ancestor::*[@ident][1]/@ident" ]
             " the default value of the\x{a}" ~
             "        @"
             sch:value-of [ select = "@ident" ]
             " attribute is not among the closed list of possible\x{a}" ~
             "        values"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.identified.attributes,
    ## specifies the optionality of the attribute.
    [ a:defaultValue = "opt" ]
    attribute usage {
      ## (required) 
      "req" | ## (recommended ) 
        "rec" | ## (optional ) 
        "opt"
    }?,
    ## (namespace) specifies the namespace to which this attribute belongs
    [ a:defaultValue = "http://www.tei-c.org/ns/1.0" ]
    attribute ns { xsd:anyURI }?,
    empty
  }

standoff-proposal_attRef =
  ## (attribute pointer) points to the definition of an attribute or group of attributes. [22.2. Modules and Schemas]
  element tei:attRef {
    empty,
    standoff-proposal_att.global.attributes,
    ## the name of the attribute class
    attribute class {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    ## the name of the attribute
    attribute name { xsd:string }?,
    empty
  }

standoff-proposal_datatype =
  ## specifies the declared value for an attribute, by referring to any datatype defined by the chosen schema language. [1.4.2. Datatype Specifications 22.5.3. Attribute List Specification]
  element tei:datatype {
    (standoff-proposal_dataRef | anyElement-datatype),
    standoff-proposal_att.global.attributes,
    ## (minimum number of occurences) indicates the minimum number of times this datatype may occur in the specification of the attribute being defined
    [ a:defaultValue = "1" ]
    attribute minOccurs { xsd:nonNegativeInteger }?,
    ## (maximum number of occurences) indicates the maximum number of times this datatype may occur in the specification of the attribute being defined
    [ a:defaultValue = "1" ]
    attribute maxOccurs {
      xsd:nonNegativeInteger
      | (
         "unbounded")
    }?,
    empty
  }

standoff-proposal_dataRef =
  ## identifies the datatype of an attribute value, either by referencing an item in an externally defined datatype library, or by pointing to a TEI-defined data specification [22.5.3.1. Datatypes]
  element tei:dataRef {
    (standoff-proposal_dataFacet*)
    >> sch:pattern [
         id =
           "standoff-proposal-dataRef-restrictDataFacet-constraint-rule-26"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:dataRef[tei:dataFacet]"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@name"
             role = "nonfatal"
             "Data facets can only be specified for references to datatypes specified by\x{a}" ~
             "          XML Schemas: Part 2: Datatypes"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-dataRef-restrictAttRestriction-constraint-rule-27"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:dataRef[tei:dataFacet]"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@restriction"
             role = "nonfatal"
             "The attribute restriction cannot be used when dataFacet elements are present."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    (
     ## the identifier used for this datatype specification
     attribute key { xsd:NCName }? | ## the name of a datatype in the list provided by XML Schemas: Part 2: Datatypes
       attribute name { xsd:NCName }? | ## a pointer to a datatype defined in some datatype library
       attribute ref { xsd:anyURI }?),
    ## supplies a string representing a regular expression providing additional constraints on the strings used to represent values of this datatype
    attribute restriction { xsd:token }?,
    empty
  }

standoff-proposal_dataFacet =
  ## Restricts the value of the strings used to represent values of a datatype, according to XML Schemas: Part 2: Datatypes. [22.5.3.1. Datatypes]
  element tei:dataFacet {
    empty,
    standoff-proposal_att.global.attributes,
    ## the name of the facet.
    attribute name {
      "length" | "minLength" | "maxLength" | "pattern" | "enumeration" | "whiteSpace" | "maxInclusive" | "minInclusive" | "maxExclusive" | "minExclusive" | "totalDigits" | "fractionDigits"
    },
    ## the facet value.
    attribute value { xsd:string },
    empty
  }

standoff-proposal_defaultVal =
  ## (default value) specifies the default declared value for an attribute. [22.5.3. Attribute List Specification]
  element tei:defaultVal {
    text,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.deprecated.attributes,
    empty
  }

standoff-proposal_valDesc =
  ## (value description) specifies any semantic or syntactic constraint on the value that an attribute may take, additional to the information carried by the datatype element. [22.5.3. Attribute List Specification]
  element tei:valDesc {
    standoff-proposal_macro.phraseSeq.limited,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.translatable.attributes,
    standoff-proposal_att.combinable.attributes,
    empty
  }

standoff-proposal_valItem =
  ## documents a single value in a predefined list of values. [22.5.3. Attribute List Specification]
  element tei:valItem {
    (standoff-proposal_model.glossLike*,
     (standoff-proposal_model.descLike+,
      (standoff-proposal_model.glossLike,
       standoff-proposal_model.descLike*)*,
      standoff-proposal_remarks*)?,
     standoff-proposal_paramList?),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.combinable.attributes,
    ## specifies the value concerned.
    attribute ident { xsd:string },
    empty
  }

standoff-proposal_valList =
  ## (value list) contains one or more valItem elements defining possible values. [22.5.3. Attribute List Specification]
  element tei:valList {
    standoff-proposal_valItem*,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.combinable.attributes,
    ## specifies the extensibility of the list of values specified.
    [ a:defaultValue = "open" ]
    attribute type {
      ## only the values specified are permitted.
      "closed" | ## (semi-open) all the values specified should be supported, but other values are legal and software should have appropriate fallback processing for them.
        "semi" | ## the values specified are sample values only.
        "open"
    }?,
    empty
  }

standoff-proposal_textNode =
  ## indicates the presence of a text node in a content model [22. Documentation Elements]
  element tei:textNode {
    empty, standoff-proposal_att.global.attributes, empty
  }

standoff-proposal_anyElement =
  ## indicates the presence of any elements in a content model [22. Documentation Elements]
  element tei:anyElement {
    empty,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.repeatable.attributes,
    ## supplies a list of namespaces to one of which the permitted elements must belong. 
    attribute require {
      list { xsd:anyURI+ }
    }?,
    ## supplies a list of namespaces or prefixed element names which are not permitted.
    attribute except {
      list {
        (xsd:anyURI { pattern = "[^/]+:.*" }
         | xsd:Name { pattern = ".+:.+" })+
      }
    }?,
    empty
  }

standoff-proposal_empty =
  ## indicates the presence of an empty node within a content model [22. Documentation Elements]
  element tei:empty {
    empty, standoff-proposal_att.global.attributes, empty
  }

standoff-proposal_standOff =
  ## Container element for linked data, contextual information, and stand-off annotations embedded in a TEI document
  element standOff {
    (standoff-proposal_model.standOffPart+)
    >> sch:ns [
         prefix = "prop"
         uri = "http://www.tei-c.org/proposal/standoff/ns"
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-standOff-nested_standOff_should_be_typed-constraint-assert-41"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "prop:standOff"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@type or not(ancestor::prop:standOff)"
             "This "
             sch:name [ ]
             " element must have a @type attribute, since it is nested inside a "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.declaring.attributes,
    empty
  }

standoff-proposal_extraTextual =
  ## Container element for extra textual phenomena.
  element extraTextual {
    (standoff-proposal_acrostic | standoff-proposal_anagram)+,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    empty
  }

standoff-proposal_acrostic =
  ## a derived element that contains the word or phrase (including any appropriate phrase-level encoding like persName) that are spelled by the initial letters of a passage (e.g., the first letter of each line of a stanza or poem).
  element acrostic {
    standoff-proposal_macro.phraseSeq
    >> sch:pattern [
         id =
           "standoff-proposal-acrostic-acrostic_attrs-constraint-report-40"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "prop:acrostic"
           "\x{a}" ~
           "               "
           sch:report [
             test = "(@target|@match) and @components"
             "Either @target, @target and @match, or @components should be used to indicate the acrostic, not both @target and @components"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-acrostic-acrostic_attrs-constraint-report-41"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "prop:acrostic"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "@match[not(substring(normalize-space(.),1,1) eq '/')] and not(@target)"
             "The @target attribute must be specified to indicate the context for @match"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.scoping.attribute.match,
    ## points to the parent element (e.g., an lg) that contains the elements of which the first letter of each make up the acrostic.
    attribute target { xsd:anyURI }?,
    ## points to a sequence of individual elements; when taken in the order specified, the first letter of each element that has textual content or the first letter immediately following each that is empty, make up the acrostic.
    attribute components {
      list { xsd:anyURI, xsd:anyURI, xsd:anyURI* }
    }?,
    empty
  }

standoff-proposal_anagram =
  ## indicates that two or more strings (either words or phrases) are anagrams of each other
  element anagram {
    standoff-proposal_macro.phraseSeq,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.pointing.attribute.targetLang,
    standoff-proposal_att.pointing.attribute.evaluate,
    ## points to an element of which the current element is (roughly) an anagram, or points to 2 or more elements whose content are (roughly) anagrams of each other
    attribute target {
      list { xsd:anyURI+ }
    }
    >> sch:pattern [
         id =
           "standoff-proposal-anagram-target-one_ptr_iff_content-constraint-rule-28"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:anagram[normalize-space(.) eq '']"
           "\x{a}" ~
           "		             "
           sch:let [
             name = "targets"
             value = "tokenize( normalize-space( @target ),' ')"
           ]
           "\x{a}" ~
           "		             "
           sch:assert [
             test = "count( $targets ) gt 1"
             "An empty ＜anagram＞ element should point to 2 or more targets"
           ]
           "\x{a}" ~
           "		          "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-anagram-target-one_ptr_iff_content-constraint-rule-29"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:anagram[normalize-space(.) ne '']"
           "\x{a}" ~
           "		             "
           sch:let [
             name = "targets"
             value = "tokenize( normalize-space( @target ),' ')"
           ]
           "\x{a}" ~
           "		             "
           sch:assert [
             test = "count( $targets ) eq 1"
             "An ＜anagram＞ element that contains an anagram should point to only 1 target"
           ]
           "\x{a}" ~
           "		          "
         ]
         "\x{a}" ~
         "         "
       ],
    empty
  }

standoff-proposal_listAnnotation =
  ## Groups together annotations, for instance for parallel interpretations of a spoken segment
  element listAnnotation {
    (standoff-proposal_model.headLike*,
     (standoff-proposal_annotationBlock
      | standoff-proposal_annotation
      | standoff-proposal_model.noteLike)+),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.notated.attributes,
    standoff-proposal_att.timed.attributes,
    standoff-proposal_att.declaring.attributes,
    empty
  }

standoff-proposal_annotation =
  element annotation {
    empty,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.pointing.attributes,
    ## pointer(s) to the annotation body
    attribute body {
      list { xsd:anyURI+ }
    },
    standoff-proposal_att.scoping.attribute.match,
    empty
  }
standoff-proposal_model.standOffPart =
  standoff-proposal_model.global.meta
  | standoff-proposal_model.biblLike
  | standoff-proposal_model.listLike
  | standoff-proposal_model.entryLike
  | standoff-proposal_castList
  | standoff-proposal_listChange
  | standoff-proposal_seg
  | standoff-proposal_graph
  | standoff-proposal_tree
  | standoff-proposal_eTree
  | standoff-proposal_forest
  | standoff-proposal_listForest
  | standoff-proposal_u
  | standoff-proposal_annotationBlock
  | standoff-proposal_zone
  | standoff-proposal_extraTextual
  | standoff-proposal_listAnnotation
standoff-proposal_model.OATarget = standoff-proposal_span
standoff-proposal_model.OABody =
  standoff-proposal_model.noteLike
  | standoff-proposal_measure
  | standoff-proposal_date
  | standoff-proposal_org
  | standoff-proposal_person
  | standoff-proposal_place
start = standoff-proposal_TEI
