         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.transcriptional.attributes,
    standoff-proposal_att.placement.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.spanning.attributes,
    standoff-proposal_att.dimensions.attributes,
    empty
  }

