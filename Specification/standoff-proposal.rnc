  element tei:org {
    (standoff-proposal_model.headLike*,
     (standoff-proposal_model.pLike*
      | (standoff-proposal_model.labelLike
         | standoff-proposal_model.nameLike
         | standoff-proposal_model.placeLike
         | standoff-proposal_model.orgPart
         | standoff-proposal_model.milestoneLike)*),
     (standoff-proposal_model.noteLike
      | standoff-proposal_model.biblLike
      | standoff-proposal_linkGrp
      | standoff-proposal_link)*,
     standoff-proposal_model.personLike*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.sortable.attributes,
    ## specifies a primary role or classification for the organization.
    attribute role {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    empty
  }

standoff-proposal_listRelation =
  ## provides information about relationships identified amongst people, places, and organizations, either informally as prose or as formally expressed relation links. [13.3.2.3. Personal Relationships]
  element tei:listRelation {
    (standoff-proposal_model.headLike*,
     (standoff-proposal_model.pLike
      | (standoff-proposal_relation
         | standoff-proposal_listRelation)+)),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.sortable.attributes,
    empty
  }

standoff-proposal_person =
  ## provides information about an identifiable individual, for example a participant in a language interaction, or a person referred to in a historical source. [13.3.2. The Person Element 15.2.2. The Participant Description]
  element tei:person {
    (standoff-proposal_model.pLike+
     | (standoff-proposal_model.personPart
        | standoff-proposal_model.global)*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.sortable.attributes,
    ## specifies a primary role or classification for the person.
    attribute role {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    ## specifies the sex of the person.
    attribute sex {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    ## specifies an age group for the person.
    attribute age {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }

standoff-proposal_persona =
  ## provides information about one of the personalities identified for a given individual, where an individual has multiple personalities. [13.3.2. The Person Element]
  element tei:persona {
    (standoff-proposal_model.pLike+
     | (standoff-proposal_model.personPart
        | standoff-proposal_model.global)*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.sortable.attributes,
    ## specifies a primary role or classification for the persona.
    attribute role {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    ## specifies the sex of the persona.
    attribute sex {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    ## specifies an age group for the persona.
    attribute age {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }

standoff-proposal_personGrp =
  ## (personal group) describes a group of individuals treated as a single person for analytic purposes. [15.2.2. The Participant Description]
  element tei:personGrp {
    (standoff-proposal_model.pLike+
     | (standoff-proposal_model.personPart
        | standoff-proposal_model.global)*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.sortable.attributes,
    ## specifies the role of this group of participants in the interaction.
    attribute role {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    ## specifies the sex of the participant group.
    attribute sex {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    ## specifies the age group of the participants.
    attribute age {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    ## describes informally the size or approximate size of the group for example by means of a number and an indication of accuracy e.g. approx 200.
    attribute size {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    empty
  }

standoff-proposal_place =
  ## contains data about a geographic location [13.3.4. Places]
  element tei:place {
    (standoff-proposal_model.headLike*,
     (standoff-proposal_model.pLike*
      | (standoff-proposal_model.labelLike
         | standoff-proposal_model.placeStateLike
         | standoff-proposal_model.eventLike)*),
     (standoff-proposal_model.noteLike
      | standoff-proposal_model.biblLike
      | standoff-proposal_idno
      | standoff-proposal_linkGrp
      | standoff-proposal_link)*,
     (standoff-proposal_model.placeLike
      | standoff-proposal_listPlace)*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.sortable.attributes,
    empty
  }

standoff-proposal_population =
  ## contains information about the population of a place. [13.3.4.3. States, Traits, and Events]
  element tei:population {
    (standoff-proposal_precision*,
     standoff-proposal_model.headLike*,
     ((standoff-proposal_model.pLike+
       | standoff-proposal_model.labelLike+),
      (standoff-proposal_model.noteLike
       | standoff-proposal_model.biblLike)*)?,
     standoff-proposal_population*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.datable.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.naming.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.dimensions.attributes,
    empty
  }

standoff-proposal_relation =
  ## (relationship) describes any kind of relationship or linkage amongst a specified group of places, events, persons, objects or other items. [13.3.2.3. Personal Relationships]
  element tei:relation {
    (standoff-proposal_desc?)
    >> sch:pattern [
         id =
           "standoff-proposal-relation-reforkeyorname-constraint-assert-15"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:relation"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@ref or @key or @name"
             "One of the attributes  'name', 'ref' or 'key' must be supplied"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-relation-activemutual-constraint-report-23"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:relation"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@active and @mutual"
             "Only one of the attributes @active and @mutual may be supplied"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-relation-activepassive-constraint-report-24"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:relation"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@passive and not(@active)"
             "the attribute 'passive' may be supplied only if the attribute 'active' is supplied"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.datable.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.canonical.attributes,
    standoff-proposal_att.sortable.attributes,
    standoff-proposal_att.typed.attributes,
    ## supplies a name for the kind of relationship of which this is an instance.
    attribute name {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    (
     ## identifies the active participants in a non-mutual relationship, or all the participants in a mutual one.
     attribute active {
       list { xsd:anyURI+ }
     }? | ## supplies a list of participants amongst all of whom the relationship holds equally.
       attribute mutual {
         list { xsd:anyURI+ }
       }?),
    ## identifies the passive participants in a non-mutual relationship.
    attribute passive {
      list { xsd:anyURI+ }
    }?,
    empty
  }

standoff-proposal_residence =
  ## describes a person's present or past places of residence. [15.2.2. The Participant Description]
  element tei:residence {
    standoff-proposal_macro.phraseSeq,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.datable.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.naming.attributes,
    standoff-proposal_att.typed.attribute.subtype,
    ## characterizes the element in some sense, using any convenient classification scheme or typology.
    ## Sample values include: 1] primary; 2] secondary; 3] temporary; 4] permanent
    attribute type {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }

standoff-proposal_sex =
  ## specifies the sex of a person. [13.3.2.1. Personal Characteristics]
  element tei:sex {
    standoff-proposal_macro.phraseSeq,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.datable.attributes,
    standoff-proposal_att.typed.attribute.subtype,
    ## characterizes the element in some sense, using any convenient classification scheme or typology.
    ## Sample values include: 1] explicit; 2] implicit
    attribute type {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    ## supplies a coded value for sex
    attribute value {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    empty
  }

standoff-proposal_socecStatus =
  ## (socio-economic status) contains an informal description of a person's perceived social or economic status. [15.2.2. The Participant Description]
  element tei:socecStatus {
    standoff-proposal_macro.phraseSeq,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.datable.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.naming.attributes,
    standoff-proposal_att.typed.attribute.subtype,
    ## characterizes the element in some sense, using any convenient classification scheme or typology.
    ## Sample values include: 1] atBirth; 2] atDeath; 3] dependent; 4] inherited; 5] independent
    attribute type {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    ## identifies the classification system or taxonomy in use, for example by pointing to a locally-defined taxonomy element or by supplying a URI for an externally-defined system.
    attribute scheme { xsd:anyURI }?,
    ## identifies a status code defined within the classification system or taxonomy defined by the scheme attribute.
    attribute code { xsd:anyURI }?,
    empty
  }

standoff-proposal_state =
  ## contains a description of some status or quality attributed to a person, place, or organization often at some specific time or for a specific date range. [13.3.1. Basic Principles 13.3.2.1. Personal Characteristics]
  element tei:state {
    (standoff-proposal_precision*,
     (standoff-proposal_state+
      | (standoff-proposal_model.headLike*,
         standoff-proposal_model.pLike+,
         (standoff-proposal_model.noteLike
          | standoff-proposal_model.biblLike)*)
      | (standoff-proposal_model.labelLike
         | standoff-proposal_model.noteLike
         | standoff-proposal_model.biblLike)*)),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.datable.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.naming.attributes,
    standoff-proposal_att.dimensions.attributes,
    empty
  }

standoff-proposal_terrain =
  ## contains information about the physical terrain of a place. [13.3.4.3. States, Traits, and Events]
  element tei:terrain {
    (standoff-proposal_precision*,
     standoff-proposal_model.headLike*,
     (standoff-proposal_model.pLike+
      | standoff-proposal_model.labelLike+),
     (standoff-proposal_model.noteLike
      | standoff-proposal_model.biblLike)*,
     standoff-proposal_terrain*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.datable.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.naming.attributes,
    standoff-proposal_att.typed.attributes,
    empty
  }

standoff-proposal_trait =
  ## contains a description of some status or quality attributed to a person, place, or organization typically, but not necessarily, independent of the volition or action of the holder and usually not at some specific time or for a specific date range. [13.3.1. Basic Principles 13.3.2.1. Personal Characteristics]
  element tei:trait {
    (standoff-proposal_precision*,
     (standoff-proposal_trait+
      | (standoff-proposal_model.headLike*,
         standoff-proposal_model.pLike+,
         (standoff-proposal_model.noteLike
          | standoff-proposal_model.biblLike)*)
      | (standoff-proposal_model.labelLike
         | standoff-proposal_model.noteLike
         | standoff-proposal_model.biblLike)*)),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.datable.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.naming.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.dimensions.attributes,
    empty
  }

standoff-proposal_objectName =
  ## (name of an object) contains a proper noun or noun phrase used to refer to an object. [13.2.4. Object Names]
  element tei:objectName {
    standoff-proposal_macro.phraseSeq,
    standoff-proposal_att.datable.attributes,
    standoff-proposal_att.editLike.attributes,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.personal.attributes,
    standoff-proposal_att.typed.attributes,
    empty
  }

standoff-proposal_object =
  ## contains a description of a single identifiable physical object. [13.3.5. Objects]
  element tei:object {
    (standoff-proposal_objectIdentifier+,
     standoff-proposal_model.headLike*,
     (standoff-proposal_model.pLike*
      | (standoff-proposal_msContents?,
         standoff-proposal_physDesc?,
         standoff-proposal_history?,
         standoff-proposal_additional?)),
     (standoff-proposal_model.noteLike
      | standoff-proposal_model.biblLike
      | standoff-proposal_linkGrp
      | standoff-proposal_link)*,
     standoff-proposal_object?),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.sortable.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.declaring.attributes,
    standoff-proposal_att.docStatus.attributes,
    standoff-proposal_att.canonical.attributes,
    empty
  }

standoff-proposal_listObject =
  ## (list of objects) contains a list of descriptions, each of which provides information about an identifiable physical object. [13.3.5. Objects]
  element tei:listObject {
    (standoff-proposal_model.headLike*,
     standoff-proposal_model.objectLike+,
     (standoff-proposal_relation | standoff-proposal_listRelation)*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.declarable.attributes,
    standoff-proposal_att.sortable.attributes,
    empty
  }

standoff-proposal_objectIdentifier =
  ## (object identifier) groups one or more identifiers or pieces of locating information concerning a single object. [13.3.5. Objects]
  element tei:objectIdentifier {
    ((standoff-proposal_model.placeNamePart
      | standoff-proposal_institution
      | standoff-proposal_repository
      | standoff-proposal_collection
      | standoff-proposal_idno
      | standoff-proposal_msName
      | standoff-proposal_objectName
      | standoff-proposal_altIdentifier
      | standoff-proposal_address)*)
    >> sch:pattern [
         id =
           "standoff-proposal-objectIdentifier-objectIdentifier_minimal-constraint-report-25"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:objectIdentifier"
           "\x{a}" ~
           "               "
           sch:report [
             test = "not(count(*) gt 0)"
             "An objectIdentifier must contain at minimum a single piece of locating or identifying information."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    empty
  }

standoff-proposal_nym =
  ## (canonical name) contains the definition for a canonical name or name component of any kind. [13.3.6. Names and Nyms]
  element tei:nym {
    (standoff-proposal_model.entryPart*,
     standoff-proposal_model.pLike*,
     standoff-proposal_nym*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.sortable.attributes,
    ## points to constituent nyms
    attribute parts {
      list { xsd:anyURI+ }
    }?,
    empty
  }

standoff-proposal_listNym =
  ## (list of canonical names) contains a list of nyms, that is, standardized names for any thing. [13.3.6. Names and Nyms]
  element tei:listNym {
    (standoff-proposal_model.headLike*,
     (standoff-proposal_nym | standoff-proposal_listNym)+,
     (standoff-proposal_relation | standoff-proposal_listRelation)*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.declarable.attributes,
    standoff-proposal_att.sortable.attributes,
    empty
  }

standoff-proposal_graph =
  ## encodes a graph, which is a collection of nodes, and arcs which connect the nodes. [19.1. Graphs and Digraphs]
  element tei:graph {
    ((standoff-proposal_label, standoff-proposal_model.global*)?,
     (((standoff-proposal_node, standoff-proposal_model.global*)+,
       (standoff-proposal_arc, standoff-proposal_model.global*)*)
      | ((standoff-proposal_arc, standoff-proposal_model.global*)+,
         (standoff-proposal_node, standoff-proposal_model.global*)+))),
    standoff-proposal_att.global.attributes,
    ## describes the type of graph.
    ## Suggested values include: 1] undirected; 2] directed; 3] transitionNetwork; 4] transducer
    attribute type {
      ## undirected graph
      "undirected" | ## directed graph
        "directed" | ## a directed graph with distinguished initial and final nodes
        "transitionNetwork" | ## a transition network with up to two labels on each arc
        "transducer"
      | xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    ## states the order of the graph, i.e., the number of its nodes.
    attribute order { xsd:nonNegativeInteger }?,
    ## states the size of the graph, i.e., the number of its arcs.
    attribute size { xsd:nonNegativeInteger }?,
    empty
  }

standoff-proposal_node =
  ## encodes a node, a possibly labeled point in a graph. [19.1. Graphs and Digraphs]
  element tei:node {
    (standoff-proposal_label, standoff-proposal_label?)?,
    standoff-proposal_att.global.attributes,
    ## provides the value of a node, which is a feature structure or other analytic element.
    attribute value { xsd:anyURI }?,
    ## provides a type for a node.
    ## Suggested values include: 1] initial; 2] final
    attribute type {
      ## initial node in a transition network
      "initial" | ## final node in a transition network
        "final"
      | xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    ## (adjacent to) gives the identifiers of the nodes which are adjacent to the current node.
    attribute adjTo {
      list { xsd:anyURI+ }
    }?,
    ## (adjacent from) gives the identifiers of the nodes which are adjacent from the current node.
    attribute adjFrom {
      list { xsd:anyURI+ }
    }?,
    ## (adjacent) gives the identifiers of the nodes which are both adjacent to and adjacent from the current node.
    attribute adj {
      list { xsd:anyURI+ }
    }?,
    ## gives the in degree of the node, the number of nodes which are adjacent from the given node.
    attribute inDegree { xsd:nonNegativeInteger }?,
    ## gives the out degree of the node, the number of nodes which are adjacent to the given node.
    attribute outDegree { xsd:nonNegativeInteger }?,
    ## gives the degree of the node, the number of arcs with which the node is incident.
    attribute degree { xsd:nonNegativeInteger }?,
    empty
  }

standoff-proposal_arc =
  ## encodes an arc, the connection from one node to another in a graph. [19.1. Graphs and Digraphs]
  element tei:arc {
    (standoff-proposal_label, standoff-proposal_label?)?,
    standoff-proposal_att.global.attributes,
    ## gives the identifier of the node which is adjacent from this arc.
    attribute from { xsd:anyURI },
    ## gives the identifier of the node which is adjacent to this arc.
    attribute to { xsd:anyURI },
    empty
  }

standoff-proposal_tree =
  ## encodes a tree, which is made up of a root, internal nodes, leaves, and arcs from root to leaves. [19.2. Trees]
  element tei:tree {
    (standoff-proposal_label?,
     ((standoff-proposal_leaf | standoff-proposal_iNode)*,
      standoff-proposal_root,
      (standoff-proposal_leaf | standoff-proposal_iNode)*)),
    standoff-proposal_att.global.attributes,
    ## gives the maximum number of children of the root and internal nodes of the tree.
    attribute arity { xsd:nonNegativeInteger }?,
    ## (ordered) indicates whether or not the tree is ordered, or if it is partially ordered.
    attribute ord {
      ## indicates that all of the branching nodes of the tree are ordered.
      "true" | ## indicates that some of the branching nodes of the tree are ordered and some are unordered.
        "partial" | ## indicates that all of the branching nodes of the tree are unordered.
        "false"
    },
    ## gives the order of the tree, i.e., the number of its nodes.
    attribute order { xsd:nonNegativeInteger }?,
    empty
  }

standoff-proposal_root =
  ## (root node) represents the root node of a tree. [19.2. Trees]
  element tei:root {
    standoff-proposal_label?,
    standoff-proposal_att.global.attributes,
    ## identifies the root node of the network by pointing to a feature structure or other analytic element.
    attribute value { xsd:anyURI }?,
    ## identifies the elements which are the children of the root node.
    attribute children {
      list { xsd:anyURI+ }
    },
    ## (ordered) indicates whether or not the root is ordered.
    attribute ord {
      xsd:boolean
      | (
         "unknown" | "inapplicable")
    }?,
    ## gives the out degree of the root, the number of its children.
    attribute outDegree { xsd:nonNegativeInteger }?,
    empty
  }

standoff-proposal_iNode =
  ## (intermediate (or internal) node) represents an intermediate (or internal) node of a tree. [19.2. Trees]
  element tei:iNode {
    standoff-proposal_label?,
    standoff-proposal_att.global.attributes,
    ## indicates an intermediate node, which is a feature structure or other analytic element.
    attribute value { xsd:anyURI }?,
    ## provides a list of identifiers of the elements which are the children of the intermediate node.
    attribute children {
      list { xsd:anyURI+ }
    },
    ## provides the identifier of the element which is the parent of this node.
    attribute parent { xsd:anyURI }?,
    ## (ordered) indicates whether or not the internal node is ordered.
    attribute ord {
      xsd:boolean
      | (
         "unknown" | "inapplicable")
    }?,
    ## provides the identifier of an element which this node follows.
    attribute follow { xsd:anyURI }?,
    ## gives the out degree of an intermediate node, the number of its children.
    attribute outDegree { xsd:nonNegativeInteger }?,
    empty
  }

standoff-proposal_leaf =
  ## encodes the leaves (terminal nodes) of a tree. [19.2. Trees]
  element tei:leaf {
    standoff-proposal_label?,
    standoff-proposal_att.global.attributes,
    ## provides a pointer to a feature structure or other analytic element.
    attribute value { xsd:anyURI }?,
    ## provides the identifier of parent of a leaf.
    attribute parent { xsd:anyURI }?,
    ## provides an identifier of an element which this leaf follows.
    attribute follow { xsd:anyURI }?,
    empty
  }

standoff-proposal_eTree =
  ## (embedding tree) provides an alternative to the tree element for representing ordered rooted tree structures. [19.3. Another Tree Notation]
  element tei:eTree {
    (standoff-proposal_label?,
     (standoff-proposal_eTree
      | standoff-proposal_triangle
      | standoff-proposal_eLeaf
      | standoff-proposal_model.ptrLike)*),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    ## provides the value of an embedding tree, which is a feature structure or other analytic element.
    attribute value { xsd:anyURI }?,
    empty
  }

standoff-proposal_triangle =
  ## (underspecified embedding tree, so called because of its
  ##   characteristic shape when drawn) provides for an underspecified eTree, that is, an eTree with information left out. [19.3. Another Tree Notation]
  element tei:triangle {
    (standoff-proposal_label?,
     (standoff-proposal_eTree
      | standoff-proposal_triangle
      | standoff-proposal_eLeaf)*),
    standoff-proposal_att.global.attributes,
    ## supplies a value for the triangle, in the form of the identifier of a feature structure or other analytic element.
    attribute value { xsd:anyURI }?,
    empty
  }

standoff-proposal_eLeaf =
  ## (leaf or terminal node of an embedding tree) provides explicitly for a leaf of an embedding tree, which may also be encoded with the eTree element. [19.3. Another Tree Notation]
  element tei:eLeaf {
    (standoff-proposal_label?, standoff-proposal_model.ptrLike?),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    ## indicates the value of an embedding leaf, which is a feature structure or other analytic element.
    attribute value { xsd:anyURI }?,
    empty
  }

standoff-proposal_forest =
  ## provides for groups of rooted trees. [19.3. Another Tree Notation]
  element tei:forest {
    (standoff-proposal_tree
     | standoff-proposal_eTree
     | standoff-proposal_triangle)+,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    empty
  }

standoff-proposal_listForest =
  ## provides for lists of forests. [19.3. Another Tree Notation]
  element tei:listForest {
    standoff-proposal_forest+,
    standoff-proposal_att.global.attributes,
    ## identifies the type of the forest group.
    attribute type {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }
standoff-proposal_att.duration.attributes =
  standoff-proposal_att.duration.w3c.attributes,
  standoff-proposal_att.duration.iso.attributes
standoff-proposal_model.global.spoken =
  standoff-proposal_pause
  | standoff-proposal_vocal
  | standoff-proposal_kinesic
  | standoff-proposal_incident
  | standoff-proposal_writing
  | standoff-proposal_shift
standoff-proposal_model.recordingPart =
  standoff-proposal_model.dateLike
  | standoff-proposal_media
  | standoff-proposal_respStmt
  | standoff-proposal_equipment
  | standoff-proposal_broadcast
standoff-proposal_model.divPart.spoken =
  standoff-proposal_u | standoff-proposal_annotationBlock
standoff-proposal_model.divPart.spoken_alternation =
  standoff-proposal_u | standoff-proposal_annotationBlock
standoff-proposal_model.divPart.spoken_sequence =
  standoff-proposal_u, standoff-proposal_annotationBlock
standoff-proposal_model.divPart.spoken_sequenceOptional =
  standoff-proposal_u?, standoff-proposal_annotationBlock?
standoff-proposal_model.divPart.spoken_sequenceOptionalRepeatable =
  standoff-proposal_u*, standoff-proposal_annotationBlock*
standoff-proposal_model.divPart.spoken_sequenceRepeatable =
  standoff-proposal_u+, standoff-proposal_annotationBlock+

standoff-proposal_scriptStmt =
  ## (script statement) contains a citation giving details of the script used for a spoken text. [8.2. Documenting the Source of Transcribed Speech 2.2.7. The Source Description 15.3.2. Declarable Elements]
  element tei:scriptStmt {
    (standoff-proposal_model.pLike+ | standoff-proposal_model.biblLike),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.declarable.attributes,
    empty
  }

standoff-proposal_recordingStmt =
  ## (recording statement) describes a set of recordings used as the basis for transcription of a spoken text. [8.2. Documenting the Source of Transcribed Speech 2.2.7. The Source Description]
  element tei:recordingStmt {
    (standoff-proposal_model.pLike+ | standoff-proposal_recording+),
    standoff-proposal_att.global.attributes,
    empty
  }

standoff-proposal_recording =
  ## (recording event) provides details of an audio or video recording event used as the source of a spoken text, either directly or from a public broadcast. [8.2. Documenting the Source of Transcribed Speech 15.3.2. Declarable Elements]
  element tei:recording {
    (standoff-proposal_model.recordingPart
     | standoff-proposal_model.pLike)*,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.declarable.attributes,
    standoff-proposal_att.duration.attributes,
    standoff-proposal_att.typed.attribute.subtype,
    ## the kind of recording.
    [ a:defaultValue = "audio" ]
    attribute type {
      ## audio recording
      "audio" | ## audio and video recording
        "video"
    }?,
    empty
  }

standoff-proposal_equipment =
  ## provides technical details of the equipment and media used for an audio or video recording used as the source for a spoken text. [8.2. Documenting the Source of Transcribed Speech 15.3.2. Declarable Elements]
  element tei:equipment {
    standoff-proposal_model.pLike+,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.declarable.attributes,
    empty
  }

standoff-proposal_broadcast =
  ## describes a broadcast used as the source of a spoken text. [8.2. Documenting the Source of Transcribed Speech 15.3.2. Declarable Elements]
  element tei:broadcast {
    (standoff-proposal_model.pLike+
     | standoff-proposal_model.biblLike
     | standoff-proposal_recording),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.declarable.attributes,
    empty
  }

standoff-proposal_transcriptionDesc =
  ## describes the set of transcription conventions used, particularly for spoken material. [8.2. Documenting the Source of Transcribed Speech]
  element tei:transcriptionDesc {
    (standoff-proposal_model.labelLike
     | standoff-proposal_model.ptrLike
     | standoff-proposal_model.pLike)*,
    standoff-proposal_att.global.attributes,
    ## supplies an identifier for the encoding convention, independent of any version number.
    attribute ident { xsd:Name },
    ## supplies a version number for the encoding conventions used, if any.
    attribute version {
      xsd:token {
        pattern = "[\d]+[a-z]*[\d]*(\.[\d]+[a-z]*[\d]*){0,3}"
      }
    }?,
    empty
  }

standoff-proposal_u =
  ## (utterance) contains a stretch of speech usually preceded and followed by silence or by a change of speaker. [8.3.1. Utterances]
  element tei:u {
    standoff-proposal_macro.paraContent,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.timed.attributes,
    standoff-proposal_att.declaring.attributes,
    standoff-proposal_att.ascribed.directed.attributes,
    standoff-proposal_att.notated.attributes,
    ## (transition) indicates the nature of the transition between this utterance and the previous one.
    [ a:defaultValue = "smooth" ]
    attribute trans {
      ## this utterance begins without unusual pause or rapidity.
      "smooth" | ## this utterance begins with a markedly shorter pause than normal.
        "latching" | ## this utterance begins before the previous one has finished.
        "overlap" | ## this utterance begins after a noticeable pause.
        "pause"
    }?,
    empty
  }

standoff-proposal_pause =
  ## marks a pause either between or within utterances. [8.3.2. Pausing]
  element tei:pause {
    empty,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.timed.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.ascribed.directed.attributes,
    empty
  }

standoff-proposal_vocal =
  ## marks any vocalized but not necessarily lexical phenomenon, for example voiced pauses, non-lexical backchannels, etc. [8.3.3. Vocal, Kinesic, Incident]
  element tei:vocal {
    standoff-proposal_model.descLike*,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.timed.attributes,
    standoff-proposal_att.ascribed.directed.attributes,
    standoff-proposal_att.typed.attributes,
    ## indicates whether or not the phenomenon is repeated.
    [ a:defaultValue = "false" ]
    attribute iterated {
      xsd:boolean
      | (
         "unknown" | "inapplicable")
    }?,
    empty
  }

standoff-proposal_kinesic =
  ## marks any communicative phenomenon, not necessarily vocalized, for example a gesture, frown, etc. [8.3.3. Vocal, Kinesic, Incident]
  element tei:kinesic {
    standoff-proposal_model.descLike*,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.timed.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.ascribed.directed.attributes,
    ## indicates whether or not the phenomenon is repeated.
    [ a:defaultValue = "false" ]
    attribute iterated {
      xsd:boolean
      | (
         "unknown" | "inapplicable")
    }?,
    empty
  }

standoff-proposal_incident =
  ## marks any phenomenon or occurrence, not necessarily vocalized or communicative, for example incidental noises or other events affecting communication. [8.3.3. Vocal, Kinesic, Incident]
  element tei:incident {
    standoff-proposal_model.descLike*,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.timed.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.ascribed.attributes,
    empty
  }

standoff-proposal_writing =
  ## contains a passage of written text revealed to participants in the course of a spoken text. [8.3.4. Writing]
  element tei:writing {
    standoff-proposal_macro.paraContent,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.ascribed.directed.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.timed.attributes,
    ## indicates whether the writing is revealed all at once or gradually.
    attribute gradual {
      xsd:boolean
      | (
         "unknown" | "inapplicable")
    }?,
    empty
  }

standoff-proposal_shift =
  ## marks the point at which some paralinguistic feature of a series of utterances by any one speaker changes. [8.3.6. Shifts]
  element tei:shift {
    empty
    >> sch:pattern [
         id = "standoff-proposal-shift-shiftNew-constraint-assert-16"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:shift"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "@new"
             role = "warning"
             "              \x{a}" ~
             "The @new attribute should always be supplied; use the special value\x{a}" ~
             '"normal" to indicate that the feature concerned ceases to be\x{a}' ~
             "remarkable at this point."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.ascribed.attributes,
    ## a paralinguistic feature.
    ## Suggested values include: 1] tempo; 2] loud; 3] pitch; 4] tension; 5] rhythm; 6] voice
    attribute feature {
      ## speed of utterance.
      "tempo" | ## loudness.
        "loud" | ## pitch range.
        "pitch" | ## tension or stress pattern.
        "tension" | ## rhythmic qualities.
        "rhythm" | ## voice quality.
        "voice"
      | xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    ## specifies the new state of the paralinguistic feature specified.
    [ a:defaultValue = "normal" ]
    attribute new {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }

standoff-proposal_annotationBlock =
  ## groups together various annotations, e.g. for parallel interpretations of a spoken segment. [8.4.6. Analytic Coding]
  element tei:annotationBlock {
    ((standoff-proposal_u
      | standoff-proposal_spanGrp
      | standoff-proposal_model.global.spoken)+
     | (standoff-proposal_model.OABody+,
        standoff-proposal_annotation,
        standoff-proposal_model.OATarget+)),
    standoff-proposal_att.notated.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.ascribed.attributes,
    standoff-proposal_att.timed.attributes,
    standoff-proposal_att.global.attributes,
    empty
  }
standoff-proposal_model.rdgLike = standoff-proposal_rdg
standoff-proposal_model.rdgPart =
  standoff-proposal_wit
  | standoff-proposal_witStart
  | standoff-proposal_witEnd
  | standoff-proposal_lacunaStart
  | standoff-proposal_lacunaEnd
standoff-proposal_att.rdgPart.attributes =
  standoff-proposal_att.rdgPart.attribute.wit
standoff-proposal_att.rdgPart.attribute.wit =
  ## (witness or witnesses) contains a space-delimited list of one or more sigla indicating the witnesses to this reading beginning or ending at this point.
  attribute wit {
    list { xsd:anyURI+ }
  }?
standoff-proposal_att.witnessed.attributes =
  standoff-proposal_att.witnessed.attribute.wit
standoff-proposal_att.witnessed.attribute.wit =
  ## (witness or witnesses) contains a space-delimited list of one or more pointers indicating the witnesses which attest to a given reading.
  attribute wit {
    list { xsd:anyURI+ }
  }?
standoff-proposal_att.textCritical.attributes =
  standoff-proposal_att.written.attributes,
  standoff-proposal_att.textCritical.attribute.type,
  standoff-proposal_att.textCritical.attribute.cause,
  standoff-proposal_att.textCritical.attribute.varSeq,
  standoff-proposal_att.textCritical.attribute.require
standoff-proposal_att.textCritical.attribute.type =
  ## classifies the reading according to some useful typology.
  ## Sample values include: 1] substantive; 2] orthographic
  attribute type {
    xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
  }?
standoff-proposal_att.textCritical.attribute.cause =
  ## classifies the cause for the variant reading, according to any appropriate typology of possible origins.
  ## Sample values include: 1] homeoteleuton; 2] homeoarchy; 3] paleographicConfusion; 4] haplography; 5] dittography; 6] falseEmendation
  attribute cause {
    xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
  }?
standoff-proposal_att.textCritical.attribute.varSeq =
  ## (variant sequence) provides a number indicating the position of this reading in a sequence, when there is reason to presume a sequence to the variants. 
  attribute varSeq { xsd:nonNegativeInteger }?
standoff-proposal_att.textCritical.attribute.require =
  ## points to other readings that are required when adopting the current reading or lemma.
  attribute require {
    list { xsd:anyURI+ }
  }?

standoff-proposal_app =
  ## (apparatus entry) contains one entry in a critical apparatus, with an optional lemma and usually one or more readings or notes on the relevant passage. [12.1.1. The Apparatus Entry]
  element tei:app {
    (standoff-proposal_lem?,
     (standoff-proposal_model.rdgLike
      | standoff-proposal_model.noteLike
      | standoff-proposal_witDetail
      | standoff-proposal_wit
      | standoff-proposal_rdgGrp)*),
    standoff-proposal_att.global.attributes,
    ## classifies the variation contained in this element according to some convenient typology.
    attribute type {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    ## identifies the beginning of the lemma in the base text.
    attribute from { xsd:anyURI }?,
    ## identifies the endpoint of the lemma in the base text.
    attribute to { xsd:anyURI }?,
    ## (location) indicates the location of the variation, when the location-referenced method of apparatus markup is used.
    attribute loc {
      list {
        xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }+
      }
    }?,
    empty
  }

standoff-proposal_listApp =
  ## (list of apparatus entries) contains a list of apparatus entries. [12.2. Linking the Apparatus to the Text]
  element tei:listApp {
    (standoff-proposal_model.headLike*,
     (standoff-proposal_app | standoff-proposal_listApp)+),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.sortable.attributes,
    standoff-proposal_att.declarable.attributes,
    standoff-proposal_att.typed.attributes,
    empty
  }

standoff-proposal_lem =
  ## (lemma) contains the lemma, or base text, of a textual variation. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element tei:lem {
    (text
     | standoff-proposal_model.divLike
     | standoff-proposal_model.divPart
     | standoff-proposal_model.gLike
     | standoff-proposal_model.phrase
     | standoff-proposal_model.inter
     | standoff-proposal_model.global
     | standoff-proposal_model.rdgPart)*,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.textCritical.attributes,
    standoff-proposal_att.witnessed.attributes,
    empty
  }

standoff-proposal_rdg =
  ## (reading) contains a single reading within a textual variation. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element tei:rdg {
    (text
     | standoff-proposal_model.divLike
     | standoff-proposal_model.divPart
     | standoff-proposal_model.gLike
     | standoff-proposal_model.phrase
     | standoff-proposal_model.inter
     | standoff-proposal_model.global
     | standoff-proposal_model.rdgPart)*,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.textCritical.attributes,
    standoff-proposal_att.witnessed.attributes,
    empty
  }

standoff-proposal_rdgGrp =
  ## (reading group) within a textual variation, groups two or more readings perceived to have a genetic relationship or other affinity. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element tei:rdgGrp {
    (((standoff-proposal_rdgGrp, standoff-proposal_wit?)
      | ((standoff-proposal_lem, standoff-proposal_wit?)?,
         (standoff-proposal_model.rdgLike, standoff-proposal_wit?))*
      | standoff-proposal_model.noteLike*
      | standoff-proposal_witDetail*)+)
    >> sch:pattern [
         id = "standoff-proposal-rdgGrp-only1lem-constraint-assert-17"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:rdgGrp"
           "\x{a}" ~
           "               "
           sch:assert [
             test = "count(tei:lem) < 2"
             "Only one <lem> element may appear within a <rdgGrp>"
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.textCritical.attributes,
    empty
  }

standoff-proposal_witDetail =
  ## (witness detail) gives further information about a particular witness, or witnesses, to a particular reading. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element tei:witDetail {
    (text
     | standoff-proposal_model.gLike
     | standoff-proposal_model.phrase
     | standoff-proposal_model.global
     | standoff-proposal_bibl)*,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.placement.attributes,
    standoff-proposal_att.pointing.attributes,
    ## (witnesses) indicates the sigil or sigla identifying the witness or witnesses to which the detail refers.
    attribute wit {
      list { xsd:anyURI+ }
    },
    ## describes the type of information given about the witness.
    attribute type {
      xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }

standoff-proposal_wit =
  ## contains a list of one or more sigla of witnesses attesting a given reading, in a textual variation. [12.1.4. Witness Information]
  element tei:wit {
    standoff-proposal_macro.phraseSeq,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.rdgPart.attributes,
    empty
  }

standoff-proposal_listWit =
  ## (witness list) lists definitions for all the witnesses referred to by a critical apparatus, optionally grouped hierarchically. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element tei:listWit {
    (standoff-proposal_model.headLike?,
     (standoff-proposal_witness | standoff-proposal_listWit)+),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.sortable.attributes,
    empty
  }

standoff-proposal_witness =
  ## contains either a description of a single witness referred to within the critical apparatus, or a list of witnesses which is to be referred to by a single sigil. [12.1. The Apparatus Entry, Readings, and Witnesses]
  element tei:witness {
    standoff-proposal_macro.limitedContent,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.sortable.attributes,
    empty
  }

standoff-proposal_witStart =
  ## (fragmented witness start) indicates the beginning, or resumption, of the text of a fragmentary witness. [12.1.5. Fragmentary Witnesses]
  element tei:witStart {
    empty,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.rdgPart.attributes,
    empty
  }

standoff-proposal_witEnd =
  ## (fragmented witness end) indicates the end, or suspension, of the text of a fragmentary witness. [12.1.5. Fragmentary Witnesses]
  element tei:witEnd {
    empty,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.rdgPart.attributes,
    empty
  }

standoff-proposal_lacunaStart =
  ## indicates the beginning of a lacuna in the text of a mostly complete textual witness. [12.1.5. Fragmentary Witnesses]
  element tei:lacunaStart {
    empty,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.rdgPart.attributes,
    empty
  }

standoff-proposal_lacunaEnd =
  ## indicates the end of a lacuna in a mostly complete textual witness. [12.1.5. Fragmentary Witnesses]
  element tei:lacunaEnd {
    empty,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.rdgPart.attributes,
    empty
  }

standoff-proposal_variantEncoding =
  ## declares the method used to encode text-critical variants. [12.1.1. The Apparatus Entry]
  element tei:variantEncoding {
    empty,
    standoff-proposal_att.global.attributes,
    ## indicates which method is used to encode the apparatus of variants.
    attribute method {
      ## apparatus uses line numbers or other canonical reference scheme referenced in a base text.
      "location-referenced" | ## apparatus indicates the precise locations of the beginning and ending of each lemma relative to a base text.
        "double-end-point" | ## alternate readings of a passage are given in parallel in the text; no notion of a base text is necessary.
        "parallel-segmentation"
    },
    ## indicates whether the apparatus appears within the running text or external to it.
    attribute location {
      ## apparatus appears within the running text.
      "internal" | ## apparatus appears outside the base text.
        "external"
    }
    >> sch:pattern [
         id =
           "standoff-proposal-variantEncoding-location-variantEncodingLocation-constraint-rule-11"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:variantEncoding"
           "\x{a}" ~
           "               "
           sch:assert [
             test =
               "(@location != 'external') or (@method != 'parallel-segmentation')"
             "\x{a}" ~
             '              The @location value "external" is inconsistent with the\x{a}' ~
             "              parallel-segmentation method of apparatus markup."
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    empty
  }

standoff-proposal_TEI =
  ## (TEI document) contains a single TEI-conformant document, combining a single TEI header with either one or more members of the model.resourceLike class, a sequence of one or more TEI elements, or both. [4. Default Text Structure 15.1. Varieties of Composite Text]
  element tei:TEI {
    (standoff-proposal_teiHeader,
     ((standoff-proposal_model.resourceLike+, standoff-proposal_TEI*)
      | standoff-proposal_TEI+))
    >> sch:ns [ prefix = "tei" uri = "http://www.tei-c.org/ns/1.0" ]
    >> sch:ns [ prefix = "xs" uri = "http://www.w3.org/2001/XMLSchema" ]
    >> sch:ns [
         prefix = "rng"
         uri = "http://relaxng.org/ns/structure/1.0"
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    ## specifies the version number of the TEI Guidelines against which this document is valid.
    attribute version {
      xsd:token { pattern = "[\d]+(\.[\d]+){0,2}" }
    }?,
    empty
  }

standoff-proposal_text =
  ## contains a single text of any kind, whether unitary or composite, for example a poem or drama, a collection of essays, a novel, a dictionary, or a corpus sample. [4. Default Text Structure 15.1. Varieties of Composite Text]
  element tei:text {
    (standoff-proposal_model.global*,
     (standoff-proposal_front, standoff-proposal_model.global*)?,
     (standoff-proposal_body | standoff-proposal_group),
     standoff-proposal_model.global*,
     (standoff-proposal_back, standoff-proposal_model.global*)?),
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.declaring.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.written.attributes,
