  }

standoff-proposal_ref =
  ## (reference) defines a reference to another location, possibly modified by additional text or comment. [3.6. Simple Links and Cross-References 16.1. Links]
  element tei:ref {
    standoff-proposal_macro.paraContent
    >> sch:pattern [
         id = "standoff-proposal-ref-refAtts-constraint-report-7"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:ref"
           "\x{a}" ~
           "               "
           sch:report [
             test = "@target and @cRef"
             "Only one of the\x{a}" ~
             "	attributes @target' and @cRef' may be supplied on "
             sch:name [ ]
             "\x{a}" ~
             "               "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.pointing.attributes,
    standoff-proposal_att.internetMedia.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.declaring.attributes,
    standoff-proposal_att.cReferencing.attributes,
    empty
  }

standoff-proposal_list =
  ## contains any sequence of items organized as a list. [3.7. Lists]
  element tei:list {
    ((standoff-proposal_model.divTop | standoff-proposal_model.global)*,
     ((standoff-proposal_item, standoff-proposal_model.global*)+
      | (standoff-proposal_headLabel?,
         standoff-proposal_headItem?,
         (standoff-proposal_label,
          standoff-proposal_model.global*,
          standoff-proposal_item,
          standoff-proposal_model.global*)+)),
     (standoff-proposal_model.divBottom,
      standoff-proposal_model.global*)*)
    >> sch:pattern [
         id =
           "standoff-proposal-list-gloss-list-must-have-labels-constraint-rule-10"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:list[@type='gloss']"
           "\x{a}" ~
           "	              "
           sch:assert [
             test = "tei:label"
             'The content of a "gloss" list should include a sequence of one or more pairs of a label element followed by an item element'
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.sortable.attributes,
    standoff-proposal_att.typed.attribute.subtype,
    ## describes the nature of the items in the list.
    ## Suggested values include: 1] gloss; 2] index; 3] instructions; 4] litany; 5] syllogism
    attribute type {
      ## each list item glosses some term or concept, which is given by a label element preceding the list item.
      "gloss" | ## each list item is an entry in an index such as the alphabetical topical index at the back of a print volume.
        "index" | ## each list item is a step in a sequence of instructions, as in a recipe.
        "instructions" | ## each list item is one of a sequence of petitions, supplications or invocations, typically in a religious ritual.
        "litany" | ## each list item is part of an argument consisting of two or more propositions and a final conclusion derived from them.
        "syllogism"
      | xsd:token { pattern = "(\p{L}|\p{N}|\p{P}|\p{S})+" }
    }?,
    empty
  }

standoff-proposal_item =
  ## contains one component of a list. [3.7. Lists 2.6. The Revision Description]
  element tei:item {
    standoff-proposal_macro.specialPara,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.sortable.attributes,
    empty
  }

standoff-proposal_label =
  ## contains any label or heading used to identify part of a text, typically but not exclusively in a list or glossary. [3.7. Lists]
  element tei:label {
    standoff-proposal_macro.phraseSeq,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.placement.attributes,
    standoff-proposal_att.written.attributes,
    empty
  }

standoff-proposal_head =
  ## (heading) contains any type of heading, for example the title of a section, or the heading of a list, glossary, manuscript description, etc. [4.2.1. Headings and Trailers]
  element tei:head {
    (text
     | standoff-proposal_lg
     | standoff-proposal_model.gLike
     | standoff-proposal_model.phrase
     | standoff-proposal_model.inter
     | standoff-proposal_model.lLike
     | standoff-proposal_model.global)*,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.placement.attributes,
    standoff-proposal_att.written.attributes,
    empty
  }

standoff-proposal_headLabel =
  ## (heading for list labels) contains the heading for the label or term column in a glossary list or similar structured list. [3.7. Lists]
  element tei:headLabel {
    standoff-proposal_macro.phraseSeq,
    standoff-proposal_att.global.attributes,
    empty
  }

standoff-proposal_headItem =
  ## (heading for list items) contains the heading for the item or gloss column in a glossary list or similar structured list. [3.7. Lists]
  element tei:headItem {
    standoff-proposal_macro.phraseSeq,
    standoff-proposal_att.global.attributes,
    empty
  }

standoff-proposal_note =
  ## contains a note or annotation. [3.8.1. Notes and Simple Annotation 2.2.6. The Notes Statement 3.11.2.8. Notes and Statement of Language 9.3.5.4. Notes within Entries]
  element tei:note {
    standoff-proposal_macro.specialPara,
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.placement.attributes,
    standoff-proposal_att.pointing.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.written.attributes,
    ## indicates whether the copy text shows the exact place of reference for the note.
    [ a:defaultValue = "true" ] attribute anchored { xsd:boolean }?,
    ## points to the end of the span to which the note is attached, if the note is not embedded in the text at that point.
    attribute targetEnd {
      list { xsd:anyURI+ }
    }?,
    empty
  }

standoff-proposal_index =
  ## (index entry) marks a location to be indexed for whatever purpose. [3.8.2. Index Entries]
  element tei:index {
