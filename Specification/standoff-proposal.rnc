    standoff-proposal_att.declaring.attributes,
    standoff-proposal_att.typed.attributes,
    empty
  }

standoff-proposal_div =
  ## (text division) contains a subdivision of the front, body, or back of a text. [4.1. Divisions of the Body]
  element tei:div {
    ((standoff-proposal_model.divTop | standoff-proposal_model.global)*,
     ((((standoff-proposal_model.divLike
         | standoff-proposal_model.divGenLike),
        standoff-proposal_model.global*)+
       | ((standoff-proposal_model.common,
           standoff-proposal_model.global*)+,
          ((standoff-proposal_model.divLike
            | standoff-proposal_model.divGenLike),
           standoff-proposal_model.global*)*)),
      (standoff-proposal_model.divBottom,
       standoff-proposal_model.global*)*)?)
    >> sch:pattern [
         id =
           "standoff-proposal-div-abstractModel-structure-l-constraint-report-26"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:div"
           "\x{a}" ~
           "               "
           sch:report [
             test = "ancestor::tei:l"
             "\x{a}" ~
             "        Abstract model violation: Lines may not contain higher-level structural elements such as div.\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ]
    >> sch:pattern [
         id =
           "standoff-proposal-div-abstractModel-structure-p-constraint-report-27"
         "\x{a}" ~
         "            "
         sch:rule [
           context = "tei:div"
           "\x{a}" ~
           "               "
           sch:report [
             test =
               "ancestor::tei:p or ancestor::tei:ab and not(ancestor::tei:floatingText)"
             "\x{a}" ~
             "        Abstract model violation: p and ab may not contain higher-level structural elements such as div.\x{a}" ~
             "      "
           ]
           "\x{a}" ~
           "            "
         ]
         "\x{a}" ~
         "         "
       ],
    standoff-proposal_att.global.attributes,
    standoff-proposal_att.divLike.attributes,
    standoff-proposal_att.typed.attributes,
    standoff-proposal_att.declaring.attributes,
    standoff-proposal_att.written.attributes,
    empty
  }

